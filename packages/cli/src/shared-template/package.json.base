{
  "type": "module",
  "scripts": {
    "dev:server": "bun src/server/index.ts",
    "dev:client": "vite",
    "dev": "concurrently \"bun src/server/index.ts\" \"vite\"",
    "build:client": "vite build",
    "build:server": "bun build src/server/index.ts --outdir dist/server --external:bev-fs --target=bun",
    "build": "bun run build:client && bun run build:server",
    "start": "bun dist/server/index.js",
    "test": "bun test ./src/server/__tests__/*.test.ts",
    "test:watch": "bun test --watch ./src/server/__tests__/*.test.ts",
    "test:coverage": "bun test --coverage ./src/server/__tests__/*.test.ts"
  },
  "dependencies": {
    "bev-fs": "latest",
    "elysia": "*",
    "@elysiajs/static": "*",
    "vue": "*",
    "vue-router": "*"
  },
  "devDependencies": {
    "vite": "*",
    "@vitejs/plugin-vue": "*",
    "typescript": "*",
    "concurrently": "*"
  }
}
