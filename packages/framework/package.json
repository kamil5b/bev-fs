{
  "name": "bev-fs",
  "version": "0.4.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc src/index.ts src/**/*.ts --outDir dist --skipLibCheck --esModuleInterop --declaration --declarationMap --module ESNext --moduleResolution node",
    "type-check": "tsc --noEmit",
    "dev": "tsc --watch",
    "test": "bun test",
    "lint": "eslint src/ --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "elysia": "*",
    "@elysiajs/static": "*",
    "vue": "*",
    "vue-router": "*"
  },
  "devDependencies": {
    "typescript": "*",
    "@types/node": "*",
    "@types/bun": "*"
  }
}
